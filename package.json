{
  "name": "we-better",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,scss,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,scss,md,json}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "optimize-svgs": "svgo -f public/assets/images/logo --config svgo.config.cjs",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@react-spring/three": "^9.7.1",
    "@react-three/drei": "^9.80.0",
    "@react-three/fiber": "^8.13.4",
    "@supabase/supabase-js": "^2.49.1",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-query-devtools": "^5.62.7",
    "@testing-library/react": "^16.1.0",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.0.8",
    "gsap": "^3.12.7",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.344.0",
    "next-images": "^1.8.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.1",
    "react-router-dom": "^7.0.2",
    "react-toastify": "^11.0.5",
    "three": "^0.154.0",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/next": "^8.0.7",
    "@types/node": "^22.10.2",
    "@types/node-fetch": "^2.6.12",
    "@types/ps-node": "^0.1.3",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.171.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.5.1",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "ps-node": "^0.1.6",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
